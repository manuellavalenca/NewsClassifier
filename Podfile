# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'NewsClassifier' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for NewsClassifier

  pod 'DocumentClassifier', '~> 1.1.0'

  target 'NewsClassifierTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'NewsClassifierUITests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'NewsClassifierMac' do
    inherit! :search_paths
    use_frameworks!
    pod 'DocumentClassifier', '~> 1.1.0'
  end

end
